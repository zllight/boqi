
    * {
        margin: 0;
        padding: 0;
        text-decoration: none;
        list-style: none;
    }

    a {
        color: black;
    }

    .header {
        width: 1519px;
        height: 36px;
        border-bottom: 1px solid #ddd;
    }

    .header-auto {
        width: 990px;
        margin: 0 auto;
        position: relative;
    }

    .left {
        display: inline;
        float: left;
    }

    .header-left li{
        float: left;
        margin: 0 20px 0 0;
        line-height: 36px;
    }
    .right {
    display: inline;
    float: right;
}
.header-right li{
    float: left;
    height: 16px;
    padding: 0 13px;
    margin: 10px 0 0;
    border-left: 1px solid #999;
    position: relative;
    z-index: 10;
    font: 14px/14px "Microsoft YaHei";
}
.login-contant{
